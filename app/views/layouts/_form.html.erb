<%= form_for([@tray, tray.plantdats.build]) do |f| %>

    <table class="table table-striped table-hover ">
      <thead>
      <tr>
        <th>ID</th>
        <th>CURRENT</th>
        <th>MIN TEMP</th>
        <th>MAX TEMP</th>
      </tr>
      </thead>
      <tbody>
      <tr>
        <td>AMBIENT Temp</td>
        <td><%= f.text_field :ambtemp %></td>
        <td><%= f.text_field :ambtempmin %></td>
        <td><%= f.text_field :ambtembmax %></td>
      </tr>
      <tr>
        <td>FOLIAR Temp</td>
        <td><%= f.text_field :foltempcur %></td>
        <td><%= f.text_field :foltempmin %></td>
        <td><%= f.text_field :foltempmax %></td>
      </tr>
      <thead>
      <tr>
        <th>pH</th>
        <th>HEATMAT</th>
        <th>H2O2</th>
        <th>TDS</th>
      </tr>
      </thead>

      <tr>
        <td><%= f.text_field :ph %></td>
        <td><%= f.text_field :heatmat %></td>
        <td><%= f.text_field :h202 %></td>
        <td><%= f.number_field :tds %></td>
      </tr>
      </tbody>
    </table>
    <%= f.submit class: 'btn btn-raised hoverable btn-success' %></br>

<% end %>