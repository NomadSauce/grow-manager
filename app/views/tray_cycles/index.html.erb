<p id="notice"><%= notice %></p>

<h1>Listing Tray Cycles</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Tray</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tray_cycles.each do |tray_cycle| %>
      <tr>
        <td><%= tray_cycle.title %></td>
        <td><%= tray_cycle.tray %></td>
        <td><%= link_to 'Show', tray_cycle %></td>
        <td><%= link_to 'Edit', edit_tray_cycle_path(tray_cycle) %></td>
        <td><%= link_to 'Destroy', tray_cycle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tray cycle', new_tray_cycle_path %>
